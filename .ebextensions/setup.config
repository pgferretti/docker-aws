option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "docrabitt.settings"
    "PYTHONPATH": "/opt/python/current/app/docrabitt:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: docrabitt/docrabitt/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
    "/media/": "www/media/"